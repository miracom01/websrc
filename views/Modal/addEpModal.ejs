<div id="addEpModal" name="addEpModal" class="modal fade" role="dialog" aria-labelledby="gridSystemModalLabel" aria-hidden="true">
  <div class="modal-dialog">

    <!-- Modal content-->
    <form class="form-horizontal" method="post" id="addApform" name ="addApform">
      <div class="modal-content">
         <div class="modal-header">
           <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
           <h3 class="modal-title" id="gridSystemModalLabel">AP 신규 등록</h3>
         </div>
         <div class="modal-body">
           <div class="container-fluid">
             <div class="row">
               <div class="col-sm-9">
                 <h4> AP / EP 정보 </h4>
                 <div class="form-group" style="display:none;">
                   <input type="text" id="txtInputAPEPType" name="txtInputAPEPType">
                 </div>
                 <div class="btn-group" role="group">
                  <button type="button" class="btn btn-default" id="btn_addEpModal_AP">AP</button>
                  <button type="button" class="btn btn-default" id="btn_addEpModal_EP">EP</button>
                </div>
                 <div class="form-group">
                   <label for="txtInputApName">기기명:</label>
                   <input type="text" class="form-control" id="txt_addEpModal_ApEPName" name="txt_addEpModal_ApEPName">
                 </div>
                 <div class="form-group">
                   <label for="txtInputApSN">AP Serial No:</label>
                   <input type="text" class="form-control" id="txt_addEpModal_ApSN" name="txt_addEpModal_ApSN">
                 </div>
                 <div class="form-group" style="display: none;">
                   <input type="text" class="form-control" id="hidPostKey" name="hidPostKey" value="addAp">
                 </div>
               </div>
             </div>
           </div>
         </div>
         <div class="modal-footer">
           <div class="form-group">
             <button type="button" class="btn btn-default" data-dismiss="modal" id="btn_addEpMoal_Close">닫기</button>
             <button type="button" class="btn btn-primary" id="btn_addEpModal_submit">신규 등록(Ajax)</button>
           </div>
         </div>
       </div>
   </form>
  </div>
</div>
